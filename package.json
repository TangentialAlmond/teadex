{
  "name": "teadex",
  "version": "0.1",
  "description": "A full-stack application for the systematic cataloging of teas, featuring visual documentation, cultivation origins, and detailed processing workflows.",
  "main": "index.js",
  "workspaces": [
    "backend",
    "client/public-client",
    "client/admin-client",
    "client/shared"
  ],
  "scripts": {
    "dev:public": "npm run dev --workspace=client/public-client",
    "dev:backend": "npm run dev --prefix backend",
    "build": "npm install && npm run build --workspace=client/public-client",
    "start": "npm run start --prefix backend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tangentialalmond/teadex.git"
  },
  "keywords": [],
  "author": "R.H. Amanda Ng",
  "license": "AGPL-3.0-only",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/tangentialalmond/teadex/issues"
  },
  "homepage": "https://github.com/tangentialalmond/teadex#readme"
}
